class analogMemir:

    def __init__(self, analogNum, maxVolt):
        self.analogNum = analogNum

    def ToDigital(self):
        num = int((self.analogNum)*204.6)
        binary = format(num,'b')
        self.maxBit = len(binary)
        return binary

    def SetDigitalValue(self):
        digValue = self.ToDigital(self)
        self.digValue = digValue
